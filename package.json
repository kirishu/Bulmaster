{
    "name": "node-sass-build",
    "version": "0.0.0",
    "private": true,
    "devDependencies": {
        "bulma": "0.9.0",
        "node-sass": "latest"
    },
    "scripts": {
        "build-sass-normal": "node-sass -w sass/bulmaster.scss css/bulmaster.css --output-style expanded",
        "build-sass-compressed": "node-sass -w --source-map true sass/bulmaster.scss css/bulmaster.min.css --output-style compressed"
    }
}
